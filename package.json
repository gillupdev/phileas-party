{
  "name": "phileas-birthday",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node server/index.js",
    "start:dev": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build --configuration production",
    "build:full": "npm run build && npm run copy-server",
    "copy-server": "node -e \"console.log('Build complete! Files ready in server/public')\"",
    "dev:server": "node server/index.js",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^21.1.0",
    "@angular/compiler": "^21.1.0",
    "@angular/core": "^21.1.0",
    "@angular/forms": "^21.1.0",
    "@angular/platform-browser": "^21.1.0",
    "@angular/router": "^21.1.0",
    "cors": "^2.8.6",
    "dotenv": "^16.3.0",
    "express": "^4.18.2",
    "express-session": "^1.18.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.1.2",
    "@angular/cli": "^21.1.2",
    "@angular/compiler-cli": "^21.1.0",
    "typescript": "~5.9.2"
  }
}
